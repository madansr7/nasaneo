    "tags": [
        {
            "name": "feed",
            "description": "Get feeds about Near Earth Objects",
            "externalDocs": {
                "description": "Find out more",
                "url": "https://api.nasa.gov/api.html#neows-feed"
            }
        },
        {
            "name": "neo",
            "description": "Find out about all Near Earth Objects",
            "externalDocs": {
                "description": "Find out more",
                "url": "https: //api.nasa.gov/api.html#neows-browse"
            }
        },
        {
            "name": "stats",
            "description": "Get statistics on Near Earth Objects",
            "externalDocs": {
                "description": "Find stats about near earth objects",
                "url": "https: //api.nasa.gov/api.html#neows-browse"
            }
        }
    ],
    "paths": {
        "/feed": {
            "get": {
                "tags": [
                    "feed"
                ],
                "summary": "Get a list of Near Earth Objects.",
                "description": "Get a list of Near Earth Objects within a date range, The max range in one query is 7 days",
                "operationId": "feed",
                "parameters": [
                    {
                        "name": "start_date",
                        "in": "query",
                        "description": "Start of date range search, format: yyyy-MM-dd - (ex: 2015-04-28)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "end_date",
                        "in": "query",
                        "description": "End of date range search, format: yyyy-MM-dd - (ex: 2015-04-28). If left off search will extend 7 days from start_date",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "detailed",
                        "in": "query",
                        "description": "Detailed info",
                        "required": true,
                        "schema": {
                            "type": "boolean",
                            "default": "true"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Collection of near earth objects",
                                    "type": "object",
                                    "properties": {
                                        "value": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/nearearthobjectlist"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "NotFound"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/feed/today": {
            "get": {
                "tags": [
                    "feed"
                ],
                "summary": "Get a list of Near Earth Objects for today",
                "description": "Get a list of Near Earth Objects for today",
                "operationId": "feedtoday",
                "parameters": [
                    {
                        "name": "detailed",
                        "in": "query",
                        "description": "Detailed info",
                        "required": true,
                        "schema": {
                            "type": "boolean",
                            "default": "true"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/NearEarthObject"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "NotFound"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/neo/browse": {
            "get": {
                "tags": [
                    "neo"
                ],
                "summary": "Retieve a paginated list of Near Earth Objects.",
                "description": "Retieve a paginated list of Near Earth Objects",
                "operationId": "neobrowse",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "description": "size",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64",
                            "default": 20
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/NearEarthObject"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "NotFound"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/neo/{asteroid_id}": {
            "get": {
                "tags": [
                    "neo"
                ],
                "summary": "Retieve a Near Earth Objects with a given id",
                "description": "Retieve a Near Earth Objects with a given id",
                "operationId": "getByAsteroidId",
                "parameters": [
                    {
                        "name": "asteroid_id",
                        "in": "query",
                        "description": "Id of Near Earth Object - (ex: 3729835)",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "description": "size",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64",
                            "default": 20
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/NearEarthObject"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "NotFound"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        }
    },
    "externalDocs": {
        "description": "Find out about asteriods ",
        "url": "http://nasa.gov"
    },
    "components": {
        "schemas": {
            "nearearthobjectlist": {
                "type": "object",
                "properties": {
                    "links": {
                        "type": "boolean"
                    },
                    "nearearthobject": {
                        "$ref":"#/components/schemas/nearearthobject"
                    },
                    "element_count": {
                        "type": "integer",
                        "format":"Int64",
                        "description": "name of the Asteroid"
                    }
                }
            },
            "nearearthobject": {
                "type": "object",
                "properties": {
                    "is_potentially_hazardous_asteroid": {
                        "type": "boolean"
                    },
                    "neo_reference_id": {
                        "type": "string",
                        "description": "The reference ID of the Asteroid - This correlates to the SPK ID for the JPL NEO data"
                    },
                    "name": {
                        "type": "string",
                        "description": "name of the Asteroid"
                    },
                    "nasa_jpl_url": {
                        "type": "string",
                        "description": "A Link to the NASA JPL Small-Body Database website"
                    },
                    "absolute_magnitude_h": {
                        "type": "number",
                        "format": "float",
                        "description": "Is a measure of an asteroids brightness"
                    },
                    "estimated_diameter": {
                        "$ref": "#/components/schemas/estimated_diameter"
                    },
                    "close_approach_data": {
                        "type": "array",
                        "items":{
                            "$ref": "#/components/schemas/CloseApproachData"    
                        }
                    },
                    "orbital_data": {
                        "$ref": "#/components/schemas/orbital_data"
                    }
                }
            },
            "closeapproachdata": {
                "type": "object",
                "properties": {
                    "close_approach_date": {
                        "type": "string",
                        "format": "date-time",
                        "description": "approach date"
                    },
                    "epoch_date_close_approach": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "relative_velocity": {
                        "$ref": "#/components/schemas/relative_velocity"
                    },
                    "miss_distance": {
                        "$ref": "#/components/schemas/miss_distance"
                    },
                    "orbiting_body": {
                        "type": "string",
                        "description": "orbiting body object"
                    }
                },
                "description": "Close approach information of the Asteroid"
            },
            "relative_velocity": {
                "type": "object",
                "properties": {
                    "kilometers_per_second": {
                        "type": "number",
                        "format": "float"
                    },
                    "kilometers_per_hour": {
                        "type": "number",
                        "format": "float"
                    },
                    "miles_per_hour": {
                        "type": "number",
                        "format": "float"
                    }
                },
                "description": "relative velocity object"
            },
            "miss_distance": {
                "type": "object",
                "properties": {
                    "astronomical": {
                        "type": "number",
                        "format": "float"
                    },
                    "lunar": {
                        "type": "number",
                        "format": "float"
                    },
                    "kilometers": {
                        "type": "number",
                        "format": "float"
                    },
                    "miles": {
                        "type": "number",
                        "format": "float"
                    }
                },
                "description": "miss distance object"
            },
            "estimated_diameter": {
                "type": "object",
                "properties": {
                    "kilometers": {
                        "estimated_diameter_min": {
                            "type": "number",
                            "format": "float"
                        },
                        "estimated_diameter_max": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    "meters": {
                        "estimated_diameter_min": {
                            "type": "number",
                            "format": "float"
                        },
                        "estimated_diameter_max": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    "miles": {
                        "estimated_diameter_min": {
                            "type": "number",
                            "format": "float"
                        },
                        "estimated_diameter_max": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    "feet": {
                        "estimated_diameter_min": {
                            "type": "number",
                            "format": "float"
                        },
                        "estimated_diameter_max": {
                            "type": "number",
                            "format": "float"
                        }
                    }
                },
                "description": "A collection of estimated diameters \"kilometers, meters, miles, feet\""
            },
            "orbital_data": {
                "type": "object",
                "properties": {
                    "orbit_id": {
                        "type": "number",
                        "format": "float"
                    },
                    "orbit_determination_date": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "orbit_uncertainty": {
                        "type": "number",
                        "format": "float"
                    },
                    "minimum_orbit_intersection": {
                        "type": "number",
                        "format": "float"
                    },
                    "jupiter_tisserand_invariant": {
                        "type": "number",
                        "format": "float"
                    },
                    "epoch_osculation": {
                        "type": "number",
                        "format": "float"
                    },
                    "eccentricity": {
                        "type": "number",
                        "format": "float"
                    },
                    "semi_major_axis": {
                        "type": "number",
                        "format": "float"
                    },
                    "inclination": {
                        "type": "number",
                        "format": "float"
                    },
                    "ascending_node_longitude": {
                        "type": "number",
                        "format": "float"
                    },
                    "orbital_period": {
                        "type": "number",
                        "format": "float"
                    },
                    "perihelion_distance": {
                        "type": "number",
                        "format": "float"
                    },
                    "perihelion_argument": {
                        "type": "number",
                        "format": "float"
                    },
                    "aphelion_distance": {
                        "type": "number",
                        "format": "float"
                    },
                    "perihelion_time": {
                        "type": "number",
                        "format": "float"
                    },
                    "mean_anomaly": {
                        "type": "number",
                        "format": "float"
                    },
                    "mean_motion": {
                        "type": "number",
                        "format": "float"
                    },
                    "equinox": {
                        "type": "string"
                    }
                },
                "description": "Orbital information of the asteroid"
            },
            "securitySchemes": {
                "api_key": {
                    "type": "apiKey",
                    "name": "api_key",
                    "in": "query"
                }
            }
        }
    }
}